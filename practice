//Prime Number
#include <stdio.h>

int main(void) {
	int i,n,temp,flag=0;
	scanf("%d",&n);
	temp = n/2;
	for(i=2;i<=temp;i++)
	{
		if(n%i==0)
		{
			flag=1;
			break;
		}
	}
	
	if(flag==0)
		printf("%d is a prime number",n);
	else
		printf("%d is not a prime number",n);
		
	return 0;
}


//Palindrome
#include <stdio.h>

int main(void) {
	int n,temp,rev=0,rem;
	scanf("%d",&n);
	temp = n;
	while(n!=0)
	{
		rem = n%10;
		rev = (rev*10) + rem;
		n = n/10;
	}
	if(rev==temp)
		printf("%d is a palindrome",temp);
	else
		printf("Not a palindrome");
	return 0;
}

