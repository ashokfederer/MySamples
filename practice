//Prime Number
#include <stdio.h>

int main(void) {
	int i,n,temp,flag=0;
	scanf("%d",&n);
	temp = n/2;
	for(i=2;i<=temp;i++)
	{
		if(n%i==0)
		{
			flag=1;
			break;
		}
	}
	
	if(flag==0)
		printf("%d is a prime number",n);
	else
		printf("%d is not a prime number",n);
		
	return 0;
}


//Palindrome
#include <stdio.h>

int main(void) {
	int n,temp,rev=0,rem;
	scanf("%d",&n);
	temp = n;
	while(n!=0)
	{
		rem = n%10;
		rev = (rev*10) + rem;
		n = n/10;
	}
	if(rev==temp)
		printf("%d is a palindrome",temp);
	else
		printf("Not a palindrome");
	return 0;
}

//Perfect Number
#include <stdio.h>

int main(void) {
	// your code goes here
	int n,temp=0,sum=1,i;
	scanf("%d",&n);
	temp = n/2;
	for(i=2;i<=temp;i++)
	{
		if(n%i==0)
		{
			sum = sum +i;
		}
	}
	if(sum==n)
	{
		printf("Perfect Number");
	}
	else
	{
		printf("Not perfect");
	}
	return 0;
}

//Armstrong
#include <stdio.h>
#include <math.h>

int main(void) {
	int n,temp,temp2,res=0,rem,count=0,sum=1,i;
	scanf("%d",&n);
	temp = n;
	temp2 = n;
	while(n!=0)
	{
		n=n/10;
		count++;
	}
	
	while(temp!=0)
	{
		rem = temp%10;
		sum = 1;
		for(i=1;i<=count;i++)
		{
			sum = sum * rem;
		}
		res = res + sum;
		temp = temp/10;
	}
	if(res==temp2)
		printf("%d is Armstrong",temp2);
	else
		printf("Not Armstrong");
	return 0;
}

//Mode
#include<stdio.h>
int main()
{
	int a[10],i,j,count=1,mode=0,ind=0,temp=0;
	for(i=0;i<10;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<9;i++)
	{
		for(j=i+1;j<10;j++)
		{
			if(a[i]>a[j])
			{
				temp = a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
	mode = a[0];
	count = 0;
	for(i=0;i<10;)
	{
		temp = 1;
		for(j=i+1;j<10;j++)
		{
			if(a[i]==a[j])
				temp++;
			else
				break;
		}
		if(temp>count)
		{
			mode=a[i];
			count = temp;
		}
		else if(temp == count)
		{
			if(mode<a[i])
			{
				mode = a[i];
			}
		}
		i = j;
	}
	printf("%d %d",mode,count);
	
	return 0;
}

